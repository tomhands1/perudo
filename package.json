        "eslint-plugin-react-hooks": "^2.1.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "coverageThreshold": {
            "global": {
                "statements": 70,
                "branches": 70,
                "functions": 70,
                "lines": 70
            }
        },
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!src/App.jsx",
            "!src/index.jsx"
        ]
    }
}
